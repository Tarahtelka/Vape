@model VapeApp.WEB.Models.FluidViewModel
@{
    ViewBag.Title = "GetFluid";
}
    <div class="row">
        <div class="col-md-6">
            <h2>Название: @Html.DisplayFor(modelitem => Model.Name)</h2>
            <h2>Страна производитель: @Html.DisplayFor(modelitem => Model.Contry)</h2>
            <h2>Компания: @Html.DisplayFor(modelitem => Model.Company)</h2>
            <h2>Бренд: @Html.DisplayFor(modelitem => Model.Brand)</h2>
            <h2>VG/PG: @Html.DisplayFor(modelitem => Model.VG)/@Html.DisplayFor(modelitem => Model.PG)</h2>
            <h2>Вкус: @Html.DisplayFor(modelitem => Model.Flavor)</h2>
            <h2>Цена: @Html.DisplayFor(modelitem => Model.Price)</h2>
        </div>
        <div class="col-md-6">
            @Html.Action("GetImgFluid", "Fluid", new { id = Model.Id })
        </div>
    </div>


